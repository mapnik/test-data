<Map srs="epsg:4326">

    <Style name="frame">
        <Rule>
            <PointSymbolizer transform="scale(1,1)"/>
        </Rule>
    </Style>

    <Layer name="frame" srs="epsg:4326">
        <StyleName>frame</StyleName>
        <Datasource>
            <Parameter name="type">csv</Parameter>
            <Parameter name="inline">
x,y
0,0
5,0
0,5
5,5
            </Parameter>
        </Datasource>
    </Layer>

</Map>
